  </ItemGroup>
	<ItemGroup>
		<Content Update="web.config">
			<CopyToPublishDirectory>Never</CopyToPublishDirectory>
		</Content>
		<Content Update="appsettings.json">
			<CopyToPublishDirectory>Never</CopyToPublishDirectory>
		</Content>
		<Content Update="wwwroot\photos\**\*">
			<CopyToPublishDirectory>Never</CopyToPublishDirectory>
		</Content>
		<Content Update="wwwroot\photos_dev\**\*">
			<CopyToPublishDirectory>Never</CopyToPublishDirectory>
		</Content>
